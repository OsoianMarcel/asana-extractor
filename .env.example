# Asana API personal access token
ASANA_TOKEN=your_personal_access_token_here

# Asana workspace ID (required)
# Find your workspace ID at: https://app.asana.com/api/1.0/workspaces
ASANA_WORKSPACE_ID=your_workspace_id_here

# Optional configuration
EXTRACTION_INTERVAL=5m
OUTPUT_DIR=./data
WORKER_COUNT=
LOG_LEVEL=info
